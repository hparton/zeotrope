{
  "name": "zeotrope",
  "version": "1.0.1",
  "license": "ISC",
  "author": "Harry Parton <hparton@hotmail.co.uk>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hparton/zeotrope.git"
  },
  "bugs": "https://github.com/hparton/zeotrope/issues",
  "description": "A lightweight animation helper, less than 1kb with no dependencies.",
  "main": "./dist/zeotrope.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir dist && npm run minify",
    "minify": "uglifyjs --compress --output dist/zeotrope.min.js -- dist/zeotrope.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "simple",
    "lightweight",
    "animation",
    "easing",
    "1kb"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "uglify-js": "^3.0.26"
  }
}
